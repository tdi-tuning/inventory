{
    "name": "stevebauman/inventory",
    "description": "Inventory management using Laravel",
    "keywords": ["laravel", "inventory", "inventory-management"],
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "illuminate/database": "10.*",
        "illuminate/support": "10.*",
        "baum/baum": "dev-laravel-10"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*"
    },
    "archive": {
      "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Stevebauman\\Inventory\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4": {
          "Stevebauman\\Inventory\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
